<div class="answer">
  <%= simple_format answer.content %>
  <div class="createdinfo">
    <%= render_accept_section answer %>
    [
    <%= answer.created_at.to_s %>
    <%= render_user answer.user %>
    ]
  </div>
  <div class="voteinfo">
    <%= render_vote_count answer %>
    <% if can_vote? answer %>
      <%= link_to "Vote+", vote_answer_path(answer), method: :post, class: "vote" %>
    <% end %>
  </div>
</div>
